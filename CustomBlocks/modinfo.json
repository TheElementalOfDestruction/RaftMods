{
    "name": "Custom Blocks",
    "author": "Destruction",
    "description": "Adds several customizable blocks.",
    "version": "4.0.0",
    "license": "GNU AGPLv3",
    "icon": "general_assets/icon.png",
    "banner": "general_assets/banner.png",
    "gameVersion": "1.0",
    "updateUrl": "https://www.raftmodding.com/api/v1/mods/customflags/version.txt",
    "isModPermanent": true,
    "excludedFiles": [],
    "modSettings": [
        {
            "type": "keybind",
            "name": "interactKey",
            "text": "Menu Key",
            "mainDefault": "F"
        },
        {
            "type": "checkbox",
            "name": "ignoreFlagMessages",
            "text": "Disable Multiplayer Blocks",
            "default": true
        },
        {
            "type": "checkbox",
            "name": "editorEnabled",
            "text": "Enable Image Selector",
            "default": true
        },
        {
            "type": "checkbox",
            "name": "preventChanges",
            "text": "Prevent Other Users From Editing Blocks",
            "default": true
        },
        {
            "type": "checkbox",
            "name": "useMipMaps",
            "Text": "Use Game Settings For Texture Quality",
            "default": true
        }
    ]
}
